# $TSUKUBA_Copyright:
#  PLEASE DESCRIBE LICENSE AGREEMENT HERE
#  $

BINDIR=`dirname $0`

args=()
for i in "$@"; do
    case "${i}" in
        -tmp)
            args=("${args[@]}" "--Wn${i}");;
        *)
            args=("${args[@]}" "${i}");;
    esac
done

set -- "${args[@]}"

${BINDIR}/omdriver --xf -Topenmp "$@"

exit $?

