newOpts=''
for i in $@ ; do
    case "${i}" in
        -OPENMP)
            newOpts="${newOpts} -fopenmp" ;;
        *);;
    esac
done

L2X="${OMNI_HOME}/bin/F_Front ${newOpts} -I@OMNI_HOME@/include"
. ${OMNI_HOME}/bin/oml2x "$@"
